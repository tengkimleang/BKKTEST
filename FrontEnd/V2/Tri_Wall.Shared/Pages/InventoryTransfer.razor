@page "/inventorytransfer"
@inherits MvvmComponentBase<InventoryTransferViewModel>
@rendermode InteractiveWebAssembly
@* @attribute [StreamRendering] *@
<PageTitle>Inventory Transfer</PageTitle>
<AuthorizeView Context="another_name">
    <Authorized>
        <FluentGrid Spacing="2" OnBreakpointEnter="@UpdateGridSize" AdaptiveRendering="true">
        </FluentGrid>
        @if (_init)
        {
            @if (_isXs)
            {
                <InventoryTransferMobile/>
            }
            else
            {
                <InventoryTransferDefault ViewModel="ViewModel"/>
            }
        }
    </Authorized>
</AuthorizeView>