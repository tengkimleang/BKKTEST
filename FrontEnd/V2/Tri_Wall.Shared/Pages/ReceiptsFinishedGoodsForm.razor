@page "/ReceiptsFinishedGoods"
@rendermode InteractiveWebAssembly
<PageTitle>Receipts Finished Goods</PageTitle>
<AuthorizeView Context="another_name">
    <Authorized>
        <FluentGrid Spacing="2" OnBreakpointEnter="@UpdateGridSize" AdaptiveRendering="true">
        </FluentGrid>
        @if (_init)
        {
            @if (_isXs)
            {
                <ReceiptFromProductionMobile/>
            }
            else
            {
                <ReceiptFromProductionDefault/>
            }
        }
    </Authorized>
</AuthorizeView>