@page "/ReturnRequest/Mobile/Add/{DocEntry:int?}"
@using Tri_Wall.Shared.Views.ReturnRequest.MobileAppScreen.Add
@rendermode InteractiveWebAssembly
<PageTitle>Create Return</PageTitle>

<AuthorizeView Context="anotherName">
    <Authorized>
        <AddReturnRequestMobile DocEntry="DocEntry" Token="@anotherName.User.FindFirst("token")?.Value" />
    </Authorized>
</AuthorizeView>

@code{
    [Parameter] public int DocEntry { get; set; }
}