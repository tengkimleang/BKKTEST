@page "/GoodReceiptPo/Mobile/Add/{DocEntry:int?}"
@using Tri_Wall.Shared.Views.GoodReceptPo.MobileAppScreen.Add
@rendermode InteractiveWebAssembly

<PageTitle>Create Good Receipt Po</PageTitle>

<AuthorizeView Context="anotherName">
    <Authorized>
        <AddGoodsReceiptPoMobile DocEntry="DocEntry" Token="@anotherName.User.FindFirst("token")?.Value" />
    </Authorized>
</AuthorizeView>

@code{
    [Parameter] public int DocEntry { get; set; }
}